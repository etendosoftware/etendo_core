<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.openbravo.base.model.RefSearch" lazy="false" table="ad_ref_search">
		<cache usage="read-write"/>
		
		<id name="id" type="string" column="ad_ref_search_id">
            <generator class="org.openbravo.base.session.DalUUIDGenerator" />
		</id>
		
		<property name="active" type="yes_no" not-null="true" column="isactive"/>
         
        <many-to-one name="referenceObject" not-null="true" class="org.openbravo.base.model.Reference" column="ad_reference_id" />

		<many-to-one name="column" not-null="true" class="org.openbravo.base.model.Column" column="ad_column_id"/>
		
		<property name="updated"/>
		
	</class>

</hibernate-mapping>